<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Cucumber" parallel="false" thread-count="3">

    <listeners>
        <listener class-name="org.jvu.listeners.TestListener" />
        <listener class-name="org.jvu.listeners.AnnotationTransformer" />
    </listeners>

    <test name="Cucumber Runner" preserve-order="false" parallel="methods" thread-count="6" enabled="false">
        <!--        <parameter name="browser" value="chrome" />-->
        <classes>
            <class name="org.jvu.stepdefinitions.testrunner.CucumberRunner" />
        </classes>
    </test>
</suite>